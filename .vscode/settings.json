{
    "editor.rulers": [80],
    "protoc": {
        "path": "/usr/bin/protoc",
        "compile_on_save": true,
        "options": [
            "--proto_path=${workspaceRoot}/api",
            "--go_out=.",
            "--go_opt=module=github.com/vsekhar/braid",
            "--go-grpc_out=.",
            "--go-grpc_opt=module=github.com/vsekhar/braid",
        ]
    }
}